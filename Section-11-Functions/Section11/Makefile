.PHONY: clean All

All:
	@echo "----------Building project:[ ArraysAndFunctions - Debug ]----------"
	@cd "ArraysAndFunctions" && "$(MAKE)" -f  "ArraysAndFunctions.mk"
clean:
	@echo "----------Cleaning project:[ ArraysAndFunctions - Debug ]----------"
	@cd "ArraysAndFunctions" && "$(MAKE)" -f  "ArraysAndFunctions.mk" clean
